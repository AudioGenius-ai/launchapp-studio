{
  "name": "@code-pilot/feature-projects",
  "version": "0.1.0",
  "description": "Projects feature module for Code Pilot Studio",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@code-pilot/core": "workspace:*",
    "@code-pilot/feature-templates": "workspace:*",
    "@code-pilot/hooks": "workspace:*",
    "@code-pilot/types": "workspace:*",
    "@code-pilot/ui-kit": "workspace:*",
    "@code-pilot/themes": "workspace:*",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-dialog": "^2.2.2",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}